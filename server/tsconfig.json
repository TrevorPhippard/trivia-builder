{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules","controllers", "migrations", "models", "routes", "helpers", "seeders", "services", "__tests__"
  ]
}